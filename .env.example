# TrashDrop Mobile Collector Driver App - Environment Variables
# Copy this file to .env and fill in your values

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Default Location (Fallback coordinates if geolocation fails)
VITE_DEFAULT_LATITUDE=5.6037
VITE_DEFAULT_LONGITUDE=-0.1870

# API Configuration
VITE_API_URL=your_api_url

# Application Settings
VITE_APP_NAME=TrashDrop
VITE_APP_VERSION=1.0.0

# TrendiPay Gateway Configuration (Phase 4)
# Enable/disable real payment processing
VITE_ENABLE_TRENDIPAY=true

# Google Maps API Key (for navigation modals)
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# TrendiPay API Credentials (get from TrendiPay dashboard)
# Test environment: https://test-api.trendipay.com
# Production environment: https://api.trendipay.com
VITE_TRENDIPAY_API_URL=https://test-api.trendipay.com
VITE_TRENDIPAY_API_KEY=your_api_key_here
VITE_TRENDIPAY_TERMINAL_ID=73f3ba0f-7f70-4baf-8e46-f31e50bdb697
VITE_TRENDIPAY_MERCHANT_ID=your_merchant_id_here
VITE_TRENDIPAY_WEBHOOK_SECRET=your_webhook_secret_here

# Backend URL for webhook callbacks
VITE_API_URL=http://localhost:3000
