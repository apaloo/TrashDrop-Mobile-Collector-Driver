# TrashDrop Mobile Collector Driver App - Environment Variables
# Copy this file to .env and fill in your values

# Supabase Configuration
VITE_SUPABASE_URL=https://tfdedlqdsajjdjkerkli.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here


# Default Location (Fallback coordinates if geolocation fails)
VITE_DEFAULT_LATITUDE=5.6037
VITE_DEFAULT_LONGITUDE=-0.1870

# Google Maps API Key
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
google_maps_api_key=your_google_maps_api_key_here

# API Configuration (not needed for mobile app)

# Application Settings
VITE_APP_NAME=TrashDrop
VITE_APP_VERSION=1.0.0

# TrendiPay Gateway Configuration (Phase 4)
# Enable/disable real payment processing
VITE_ENABLE_TRENDIPAY=true

# TrendiPay API Credentials (get from TrendiPay dashboard)
# Test environment: https://test-api.trendipay.com
# Production environment: https://api.trendipay.com
VITE_ENABLE_TRENDIPAY=true
VITE_TRENDIPAY_API_URL=https://test-api.trendipay.com
VITE_TRENDIPAY_API_KEY=your_trendipay_api_key_here
VITE_TRENDIPAY_TERMINAL_ID=your_trendipay_terminal_id_here
VITE_TRENDIPAY_MERCHANT_ID=your_trendipay_merchant_id_here
VITE_TRENDIPAY_WEBHOOK_SECRET=your_webhook_secret_here

# Supabase Edge Functions URL for webhook callbacks (TrendiPay will send payment status updates here)
# IMPORTANT: Must be a publicly accessible HTTPS URL for TrendiPay webhooks to work
# Example: https://<project-ref>.functions.supabase.co
VITE_SUPABASE_FUNCTIONS_URL=

# Legacy / fallback base URL (used if VITE_SUPABASE_FUNCTIONS_URL is not set)
VITE_API_URL=
