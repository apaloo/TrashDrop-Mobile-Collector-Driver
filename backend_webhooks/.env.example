# Backend Webhooks (Server-side)
# IMPORTANT: Use service role key for backend, NOT the anon key

SUPABASE_URL=https://<project-ref>.supabase.co
SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Webhook Security
# Get this from your TrendiPay dashboard - used to verify webhook signatures
TRENDIPAY_WEBHOOK_SECRET=your_trendipay_webhook_secret_here

# Optional: Logging
LOG_LEVEL=info